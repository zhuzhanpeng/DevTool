<resources>
    <declare-styleable name="DynamicShapeTextView">
        <attr name="bg_normal_color" format="color"/>
        <attr name="bg_focus_color" format="color"/>
        <attr name="corner_radius" format="dimension"/>
        <attr name="normal_stroke_width" format="dimension"/>
        <attr name="focus_stroke_width" format="dimension"/>
        <attr name="stroke_color" format="color"/>
        <attr name="normal_txt_color" format="color"/>
        <attr name="focus_txt_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="DynamicShapeFLayout">
        <attr name="f_bg_normal_color" format="color"/>
        <attr name="f_bg_focus_color" format="color"/>
        <attr name="f_corner_radius" format="dimension"/>
        <attr name="f_normal_stroke_width" format="dimension"/>
        <attr name="f_focus_stroke_width" format="dimension"/>
        <attr name="f_stroke_color" format="color"/>
        <attr name="f_normal_txt_color" format="color"/>
        <attr name="f_focus_txt_color" format="color"/>
    </declare-styleable>
    <style name="dialog_style" parent="Theme.AppCompat.Light.NoActionBar">
       <item name="android:layout_width">@dimen/w_191</item>
        <item name="android:layout_height">@dimen/h_76</item>
        <item name="android:textSize">@dimen/w_33</item>
    </style>
</resources>
